#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/quemiai/node_modules/.pnpm/expo@50.0.21_@babel+core@7.28.4_@react-native+babel-preset@0.81.1_react-native@0.73.2_react@18.2.0/node_modules/expo/bin/node_modules:/workspaces/quemiai/node_modules/.pnpm/expo@50.0.21_@babel+core@7.28.4_@react-native+babel-preset@0.81.1_react-native@0.73.2_react@18.2.0/node_modules/expo/node_modules:/workspaces/quemiai/node_modules/.pnpm/expo@50.0.21_@babel+core@7.28.4_@react-native+babel-preset@0.81.1_react-native@0.73.2_react@18.2.0/node_modules:/workspaces/quemiai/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/quemiai/node_modules/.pnpm/expo@50.0.21_@babel+core@7.28.4_@react-native+babel-preset@0.81.1_react-native@0.73.2_react@18.2.0/node_modules/expo/bin/node_modules:/workspaces/quemiai/node_modules/.pnpm/expo@50.0.21_@babel+core@7.28.4_@react-native+babel-preset@0.81.1_react-native@0.73.2_react@18.2.0/node_modules/expo/node_modules:/workspaces/quemiai/node_modules/.pnpm/expo@50.0.21_@babel+core@7.28.4_@react-native+babel-preset@0.81.1_react-native@0.73.2_react@18.2.0/node_modules:/workspaces/quemiai/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../expo/bin/cli" "$@"
else
  exec node  "$basedir/../expo/bin/cli" "$@"
fi
